{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Node",
		"target": "ES2022",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"rootDir": "src",
		"outDir": "out",
		"declaration": false,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitThis": true
	},
	"include": ["src/**/function.ts"],
}
